{
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  },
  "hosting": {
    "public": "public",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      { "source": "/i/**", "function": "imgProxy" },
      { "source": "/cv/**", "function": "cvProxy" }
    ]
  }
}
